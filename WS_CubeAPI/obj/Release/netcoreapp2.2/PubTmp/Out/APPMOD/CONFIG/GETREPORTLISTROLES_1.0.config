--GETREPORTLISTROLES_1.0.config
SELECT REPORTID,REPORTNAME FROM REPORTSMASTER
WHERE STATUS = 1 AND REPORTID NOT IN (SELECT REPORTID FROM ROLEREPORTS WHERE ROLEID = @ROLEID AND STATUS=1)
ORDER BY REPORTNAME
SELECT RM.REPORTID,RM.REPORTNAME FROM REPORTSMASTER RM
INNER JOIN ROLEREPORTS RR ON RR.REPORTID = RM.REPORTID AND RR.ROLEID = @ROLEID
WHERE RM.STATUS = 1 AND RR.STATUS=1
ORDER BY REPORTNAME

