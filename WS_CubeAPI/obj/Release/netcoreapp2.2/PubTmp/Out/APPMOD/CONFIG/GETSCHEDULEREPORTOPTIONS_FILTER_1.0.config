+ISNULL(CASE WHEN ROF.REPORTPARAM='PRIORITYVALUE' THEN (CASE WHEN RF.OPTIONVALUE='1' THEN (SELECT TOP 1 LABELVALUE FROM COMMONTRANSLATIONS WHERE LANGUAGEID = @LANGUAGEID AND LABELNAME='LBLHIGH') WHEN RF.OPTIONVALUE='2' THEN (SELECT TOP 1 LABELVALUE FROM COMMONTRANSLATIONS WHERE LANGUAGEID = @LANGUAGEID AND LABELNAME='LBLMEDIUM') WHEN RF.OPTIONVALUE='3' THEN (SELECT TOP 1 LABELVALUE FROM COMMONTRANSLATIONS WHERE LANGUAGEID = @LANGUAGEID AND LABELNAME='LBLLOW') END ) ELSE NULL END ,'')
