+ISNULL(CASE WHEN RF.OPTIONID=48 THEN (SELECT ENTITYVALUE FROM ENTITYTRANSLATION WHERE LANGUAGEID=@LANGUAGEID AND PARENTID=2 AND ENTITYID=CFV.CUSTOMFIELDVALUEID  AND ENTITYTYPE='CUS_VALUE') ELSE '' END,'')
