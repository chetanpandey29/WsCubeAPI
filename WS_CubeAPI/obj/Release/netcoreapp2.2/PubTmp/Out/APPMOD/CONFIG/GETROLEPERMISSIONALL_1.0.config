SELECT A.PERMISSIONID, B.PARENTID, PERMISSIONNAME,(CASE WHEN B.PARENTID=0 THEN A.PERMISSIONID ELSE 0 END) AS IMAGENO,A.PERMISSIONVALUE  FROM ROLEPERMISSIONS A
INNER JOIN PERMISSIONS B ON A.PERMISSIONID=B.PERMISSIONID
WHERE A.ROLEID=@ROLEID
AND B.PERMISSIONID NOT IN (33,34,37)
ORDER BY DISPLAYORDER

