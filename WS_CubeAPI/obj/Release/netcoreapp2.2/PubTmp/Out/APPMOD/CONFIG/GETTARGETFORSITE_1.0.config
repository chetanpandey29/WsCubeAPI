--GETTARGETFORSITE_1.0
SELECT SITEID,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,[DEC],APPLYTOALLOBSERVERS,CAST(JAN AS VARCHAR)+','+CAST(FEB AS VARCHAR)+','+CAST(MAR AS VARCHAR)+','+CAST(APR AS VARCHAR)+','+CAST(MAY AS VARCHAR)+','+CAST(JUN AS VARCHAR)+','+CAST(JUL AS VARCHAR)+','+CAST(AUG AS VARCHAR)+','+CAST(SEP AS VARCHAR)+','+CAST(OCT AS VARCHAR)+','+CAST(NOV AS VARCHAR)+','+CAST([DEC] AS VARCHAR)[OLDTARGETVALUE] FROM TARGETSITES WHERE SITEID=@SITEID