--SELECT ENTITYID,ENTITYNAME,ENTITYVALUE FROM ENTITYTRANSLATION WHERE PARENTID=@PARENTID AND LANGUAGEID=@LANGUAGEID AND ENTITYTYPE IN('UGP_TYPE') ORDER BY ENTITYNAME,ENTITYVALUE ASC
SELECT ENTITYID,ENTITYNAME,ENTITYVALUE
FROM ENTITYTRANSLATION ET
INNER JOIN GROUPS G ON G.GROUPTYPEID=ET.PARENTID AND G.GROUPID=ET.ENTITYID
WHERE PARENTID=@PARENTID AND LANGUAGEID=@LANGUAGEID AND ENTITYTYPE ='UGP_TYPE'
AND G.STATUS!=2
ORDER BY ENTITYNAME,ENTITYVALUE ASC
