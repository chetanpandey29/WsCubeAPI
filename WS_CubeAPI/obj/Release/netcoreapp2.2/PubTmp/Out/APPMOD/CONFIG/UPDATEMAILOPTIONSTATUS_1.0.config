DECLARE @CC_TIMESTAMP DATETIME
SET @CC_TIMESTAMP=GETDATE()

UPDATE SCHEDULE SET STATUS = CASE WHEN STATUS = 1 THEN 0 WHEN STATUS = 0 THEN 1 END, UPDATEDBY=@UPDATEDBY,
UPDATEDDATE=@CC_TIMESTAMP
WHERE SCHEDULEID IN (SELECT RESULT FROM DBO.CSVTOTABLE(@SCHEDULEID))  AND SCHEDULETYPEID NOT IN(5,6,11)


INSERT INTO dbo.AUDIT(TABLENAME,TRANS,STAMP,LASTUPDATEDBY,DATA)
SELECT 'SCHEDULE','UPDATE-UPDATEMAILOPTIONSTATUS_1.0',@CC_TIMESTAMP,@UPDATEDBY,NULL
