DECLARE @CC_TIMESTAMP DATETIME
SET @CC_TIMESTAMP=GETDATE()

UPDATE [SUBCATEGORIES] SET STATUS = 1,UPDATEDBY = @USERID,UPDATEDDATE = @CC_TIMESTAMP WHERE MAINCATEGORYID=@MAINCATEGORYID AND SUBCATEGORYID IN (SELECT RESULT FROM DBO.CSVTOTABLE(@SUBCATEGORYID))

UPDATE [SUBCATEGORIES] SET STATUS = 0,UPDATEDBY = @USERID,UPDATEDDATE = @CC_TIMESTAMP
WHERE STATUS!=2 AND MAINCATEGORYID=@MAINCATEGORYID AND SUBCATEGORYID NOT IN (SELECT RESULT FROM DBO.CSVTOTABLE(@SUBCATEGORYID))

SELECT @MAINCATEGORYID

INSERT INTO dbo.AUDIT(TABLENAME,TRANS,STAMP,LASTUPDATEDBY,DATA)
SELECT 'SUBCATEGORIES','UPDATE-UPDATESUBCATEGORYASSIGNMENTS_1.0',@CC_TIMESTAMP,@USERID,NULL
