SELECT DISTINCT SUBCATEGORYID,ENTITYVALUE FROM SUBCATEGORIES S
INNER JOIN ENTITYTRANSLATION ET ON ET.PARENTID = S.MAINCATEGORYID AND ET.ENTITYID =S.SUBCATEGORYID  AND ET.ENTITYTYPE='SUB_CTG' AND ET.LANGUAGEID = 4
WHERE S.STATUS=1 AND S.MAINCATEGORYID !=0 AND MAINCATEGORYID=@MAINCATEGORYID AND S.SUBCATEGORYID IN (SELECT SUBCATEGORYID FROM CHECKLISTSETUPCATEGORYASSIGNMENTS WHERE CHECKLISTSETUPID=@CHECKLISTSETUPID AND SUBISASSIGNED=0 AND MAINCATEGORYID=@MAINCATEGORYID)
ORDER BY ENTITYVALUE

SELECT DISTINCT SUBCATEGORYID,ENTITYVALUE FROM CHECKLISTSETUPCATEGORYASSIGNMENTS CS
INNER JOIN ENTITYTRANSLATION ET ON ET.PARENTID = CS.MAINCATEGORYID AND ET.ENTITYID =CS.SUBCATEGORYID AND ET.ENTITYTYPE='SUB_CTG' AND ET.LANGUAGEID = 4
WHERE CHECKLISTSETUPID=@CHECKLISTSETUPID AND CS.MAINCATEGORYID = MAINCATEGORYID AND CS.SUBISASSIGNED=1
ORDER BY ENTITYVALUE

