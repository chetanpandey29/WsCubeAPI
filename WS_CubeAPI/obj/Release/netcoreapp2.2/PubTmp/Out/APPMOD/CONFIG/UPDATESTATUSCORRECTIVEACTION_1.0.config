DECLARE @CC_TIMESTAMP DATETIME
SET @CC_TIMESTAMP=GETDATE()

UPDATE [CORRECTIVEACTIONS] SET [STATUS] = CASE  WHEN  [STATUS] = 1 THEN 0  WHEN  [STATUS] = 0 THEN 1 END,UPDATEDBY=@UPDATEDBY,UPDATEDDATE=@CC_TIMESTAMP WHERE CORRACTIONID IN (SELECT RESULT FROM DBO.CSVtoTable(@CORRACTIONID))

SELECT @CORRACTIONID

INSERT INTO dbo.AUDIT(TABLENAME,TRANS,STAMP,LASTUPDATEDBY,DATA)
SELECT 'CORRECTIVEACTIONS','UPDATE-UPDATESTATUSCORRECTIVEACTION_1.0',@CC_TIMESTAMP,@UPDATEDBY,NULL