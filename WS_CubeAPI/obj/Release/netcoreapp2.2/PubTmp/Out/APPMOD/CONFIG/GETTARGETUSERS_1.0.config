--GETTARGETUSERS_1.0
IF EXISTS (SELECT 1 FROM TARGETUSERS WHERE SITEID=@SITEID AND USERID=@USERID)
SELECT SITEID,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,[DEC],CAST(JAN AS VARCHAR)+','+CAST(FEB AS VARCHAR)+','+CAST(MAR AS VARCHAR)+','+CAST(APR AS VARCHAR)+','+CAST(MAY AS VARCHAR)+','+CAST(JUN AS VARCHAR)+','+CAST(JUL AS VARCHAR)+','+CAST(AUG AS VARCHAR)+','+CAST(SEP AS VARCHAR)+','+CAST(OCT AS VARCHAR)+','+CAST(NOV AS VARCHAR)+','+CAST([DEC] AS VARCHAR)[OLDTARGETVALUE] FROM TARGETUSERS WHERE SITEID=@SITEID AND USERID=@USERID
ELSE
SELECT SITEID,JAN,FEB,MAR,APR,MAY,JUN,JUL,AUG,SEP,OCT,NOV,[DEC],CAST(JAN AS VARCHAR)+','+CAST(FEB AS VARCHAR)+','+CAST(MAR AS VARCHAR)+','+CAST(APR AS VARCHAR)+','+CAST(MAY AS VARCHAR)+','+CAST(JUN AS VARCHAR)+','+CAST(JUL AS VARCHAR)+','+CAST(AUG AS VARCHAR)+','+CAST(SEP AS VARCHAR)+','+CAST(OCT AS VARCHAR)+','+CAST(NOV AS VARCHAR)+','+CAST([DEC] AS VARCHAR)[OLDTARGETVALUE] FROM TARGETSITES WHERE SITEID=@SITEID