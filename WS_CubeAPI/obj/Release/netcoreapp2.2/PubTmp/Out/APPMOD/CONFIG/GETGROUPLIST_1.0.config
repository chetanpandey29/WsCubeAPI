SELECT GROUPID,ENTITYVALUE[GROUPNAME],CASE WHEN G.STATUS= 1 THEN 'LBLACTIVE' WHEN G.STATUS = 0 THEN 'LBLINACTIVE' END AS [STATUS]
FROM GROUPS G
INNER JOIN ENTITYTRANSLATION ET ON ET.ENTITYID=G.GROUPID AND ET.LANGUAGEID=@LANGUAGECODE AND ENTITYTYPE='UGP_TYPE'
WHERE GROUPTYPEID=@GROUPTYPEID AND G.STATUS !=2 ORDER BY GROUPNAME