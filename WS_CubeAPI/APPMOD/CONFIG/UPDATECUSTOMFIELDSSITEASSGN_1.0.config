DELETE FROM SITECUSTOMFIELDS WHERE CUSTOMFIELDID=@CUSTOMFIELDID AND CUSTOMFIELDVALUEID=@CUSTOMFIELDVALUEID
IF @ALLSITES =1
UPDATE CUSTOMFIELDVALUES SET ALLSITES=@ALLSITES WHERE CUSTOMFIELDID=@CUSTOMFIELDID AND CUSTOMFIELDVALUEID=@CUSTOMFIELDVALUEID
ELSE
BEGIN
UPDATE CUSTOMFIELDVALUES SET ALLSITES=0 WHERE CUSTOMFIELDID=@CUSTOMFIELDID AND CUSTOMFIELDVALUEID=@CUSTOMFIELDVALUEID
INSERT INTO SITECUSTOMFIELDS(SITEID,CUSTOMFIELDID,CUSTOMFIELDVALUEID,STATUS,UPDATEDBY,UPDATEDDATE)
SELECT RESULT,@CUSTOMFIELDID,@CUSTOMFIELDVALUEID,1 STATUS,@UPDATEDBY,GETDATE() FROM dbo.CSVTOTABLE(@SITEID)
END