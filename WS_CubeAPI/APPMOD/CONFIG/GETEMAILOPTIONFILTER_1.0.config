
SELECT SE.OPTIONID,RO.OPTIONNAME,RO.FILTERPARAM,RO.FILTERPARAMMAP,RO.REPORTPARAM,RO.WACCESS QUERY,RO.OPTIONSCATEGORYID
FROM SCHEDULEEMAILOPTIONS SE
INNER JOIN REPORTOPTIONSMASTER RO ON RO.OPTIONID=SE.OPTIONID
INNER JOIN REPORTOPTIONSCATEGORY ROC ON ROC.OPTIONSCATEGORYID=SE.OPTIONSCATEGORYID
WHERE SE.SCHEDULETYPEID=@SCHEDULETYPEID AND SE.STATUS=1
ORDER BY ROC.OPTIONSCATEGORYID

SELECT DISTINCT ROC.OPTIONSCATEGORYID,ROC.CATEGORYNAME
FROM SCHEDULEEMAILOPTIONS SE
INNER JOIN REPORTOPTIONSMASTER RO ON RO.OPTIONID=SE.OPTIONID
INNER JOIN REPORTOPTIONSCATEGORY ROC ON ROC.OPTIONSCATEGORYID=SE.OPTIONSCATEGORYID
WHERE SE.SCHEDULETYPEID=@SCHEDULETYPEID AND SE.STATUS=1
ORDER BY ROC.OPTIONSCATEGORYID

