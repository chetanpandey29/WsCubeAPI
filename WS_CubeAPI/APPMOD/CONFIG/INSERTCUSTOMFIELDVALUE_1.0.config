DECLARE @CUSTOMFIELDVALUEID INT
IF EXISTS(SELECT 1 FROM CUSTOMFIELDVALUES WHERE CUSTOMFIELDID=@CUSTOMFIELDID AND CUSTOMFIELDVALUE=@CUSTOMFIELDVALUE AND STATUS !=2)
SELECT 1
ELSE
BEGIN
INSERT INTO CUSTOMFIELDVALUES(COMPANYID,CUSTOMFIELDVALUE,CUSTOMFIELDID,STATUS,CREATEDBY,CREATEDDATE,ALLSITES)
SELECT (SELECT COMPANYID FROM COMPANY),@CUSTOMFIELDVALUE,@CUSTOMFIELDID,1,@CREATEDBY,GETDATE(),CASE WHEN @ALLSITES=1 THEN 1 ELSE 0 END
SET @CUSTOMFIELDVALUEID=@@IDENTITY
INSERT INTO ENTITYTRANSLATION(LANGUAGEID,PARENTID,ENTITYID,ENTITYNAME,ENTITYVALUE,ENTITYTYPE,CREATEDBY,CREATEDDATE,STATUS)
SELECT LANGUAGEID,@CUSTOMFIELDID,@CUSTOMFIELDVALUEID,@CUSTOMFIELDVALUE,@CUSTOMFIELDVALUE,'CUS_VALUE',1,GETDATE(),1 FROM LANGUAGES
SELECT 2
END

