SELECT DISTINCT CS.CHECKLISTSETUPID,CS.SETUPNAME
FROM CHECKLISTSETUP CS
INNER JOIN CHECKLISTSETUPCATEGORYASSIGNMENTS CSC ON CS.CHECKLISTSETUPID=CSC.CHECKLISTSETUPID
INNER JOIN CHECKLISTSETUPSITES CSS ON CS.CHECKLISTSETUPID=CSS.CHECKLISTSETUPID
WHERE CS.[STATUS] IN (1,3)
AND CSS.SITEID=@SITEID
ORDER BY SETUPNAME