DECLARE @CC_TIMESTAMP DATETIME
SET @CC_TIMESTAMP=GETDATE()

UPDATE INJURYSTATS SET STATUS = CASE WHEN STATUS = 1 THEN 0 WHEN STATUS = 0 THEN 1 END,
UPDATEDBY = @UPDATEDBY,   UPDATEDDATE = @CC_TIMESTAMP
WHERE INJURYID IN (SELECT RESULT FROM dbo.CSVtoTable(@INJURYID))
SELECT 1


INSERT INTO dbo.AUDIT(TABLENAME,TRANS,STAMP,LASTUPDATEDBY,DATA)
SELECT 'INJURYSTATS','UPDATE-UPDATEINJURYSTATSTATUS_1.0',@CC_TIMESTAMP,@UPDATEDBY,NULL
