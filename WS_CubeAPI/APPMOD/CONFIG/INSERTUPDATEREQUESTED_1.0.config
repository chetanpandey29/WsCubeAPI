IF EXISTS(SELECT 1 FROM STOPCARDENTRY_OL WHERE CARDID=@CARDID AND EDITOR=1)
	SELECT -4
ELSE
BEGIN
INSERT INTO STOPCARDENTRYUPDATEREQUEST_OL (CARDID,COMMENTS,REQUESTTYPE,CREATEDBY,CREATEDDATE) VALUES (@CARDID,@COMMENTS,@REQUESTTYPE,@CREATEDBY,GETDATE());
UPDATE STOPCARDENTRY_OL SET APPROVALSTATUS=CASE @REQUESTTYPE WHEN 1 THEN 2 WHEN 2 THEN 3 END WHERE CARDID=@CARDID;
SELECT 1
END