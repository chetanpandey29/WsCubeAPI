--UPDATE-CHANGESTATUS_SUBAREAS_1.0
DECLARE @CC_TIMESTAMP DATETIME
SET @CC_TIMESTAMP=GETDATE()

UPDATE SUBAREAS SET [STATUS] = CASE  WHEN  [STATUS] = 1 THEN 0  WHEN  [STATUS] = 0 THEN 1 END,UPDATEDBY=@USERID,UPDATEDDATE=GETDATE()
WHERE SUBAREAID IN (SELECT RESULT FROM DBO.CSVtoTable(@SUBAREAID))

INSERT INTO dbo.AUDIT(TABLENAME,TRANS,STAMP,LASTUPDATEDBY,DATA)
SELECT 'SUBAREAS','UPDATE-CHANGESTATUS_SUBAREAS_1.0',@CC_TIMESTAMP,@USERID,(SELECT SUBAREAID,STATUS FROM SUBAREAS WHERE UPDATEDBY=@USERID AND UPDATEDDATE=@CC_TIMESTAMP FOR XML AUTO)


