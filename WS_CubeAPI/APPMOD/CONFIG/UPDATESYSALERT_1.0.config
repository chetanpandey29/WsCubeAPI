IF EXISTS(SELECT 1 FROM SYSALERT WHERE ALERTID!=@ALERTID AND ALERTNAME=@ALERTNAME AND [STATUS]!=2 ) SELECT -1
BEGIN
UPDATE SYSALERT SET ALERTNAME=@ALERTNAME,ALERTTEXT=@ALERTTEXT,[STATUS]=@STATUS,UPDATEDBY=@UPDATEDBY,UPDATEDDATE=GETDATE() WHERE ALERTID=@ALERTID
IF @STATUS=1
UPDATE SYSALERT SET STATUS=0 WHERE ALERTID!=@ALERTID;
END
SELECT 2
